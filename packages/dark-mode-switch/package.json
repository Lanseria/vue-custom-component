    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^16.6.2",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vite-dts": "^1.0.3",
    "vue-tsc": "^0.2.2"
  }
